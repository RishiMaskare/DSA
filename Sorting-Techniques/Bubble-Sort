#include<iostream>
#include<vector>
using namespace std;
void bubble_sort(vector<int>& arr, int n){
    for(int i=0;i<n-1;i++){
        bool isSwap = false;
        for(int j=0;j<n-i-1;j++){
            if(arr[j]>arr[j+1]){
                swap(arr[j], arr[j+1]);
                isSwap = true;
            }
        }
        if(!isSwap) return;
    }
}
void printArray(vector<int> arr, int n){
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
}
int main(){
    vector<int> arr = {2, 0, 2, 1, 1, 0};
    int n = arr.size();
    bubble_sort(arr, n);
    printArray(arr, n);
    return 0;
}
