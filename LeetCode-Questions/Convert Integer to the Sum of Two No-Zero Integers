class Solution {
public:
    bool hasNoZero(int n) {
        string s = to_string(n);
        for(char ch : s) {
            if(ch == '0') return false;
        }
        return true;
    }
    vector<int> getNoZeroIntegers(int n) {
        for(int i=1;i<n;i++) {
            int x = n-i;
            if(hasNoZero(x) && hasNoZero(i)) {
                return {x, i};
            }
        }
        return {};
    }
};
